<p id="notice"><%= notice %></p>

<h1>Listing Admin Boxes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_boxes.each do |admin_box| %>
      <tr>
        <td><%= link_to 'Show', admin_box %></td>
        <td><%= link_to 'Edit', edit_admin_box_path(admin_box) %></td>
        <td><%= link_to 'Destroy', admin_box, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Box', new_admin_box_path %>
