<p id="notice"><%= notice %></p>

<h1>Listing Manager Boxes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @manager_boxes.each do |manager_box| %>
      <tr>
        <td><%= link_to 'Show', manager_box %></td>
        <td><%= link_to 'Edit', edit_manager_box_path(manager_box) %></td>
        <td><%= link_to 'Destroy', manager_box, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Box', new_manager_box_path %>
